{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "arrow-parens":"off",
    "comma-dangle": "off",
    "indent": "off",
    "require-jsdoc": "off",
    "no-console": "warn"
  },
  "extends": ["eslint:recommended", "google"]
}