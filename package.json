{
  "name": "excel-course",
  "version": "1.0.0",
  "description": "Pure JS tutorial course",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test: watch": "jest --watch",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "lint": "npx eslint ./src"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "browserslist": "> 0.25%, not dead",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxBocharnikov/excel-course.git"
  },
  "author": "Max Bocharnikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaxBocharnikov/excel-course/issues"
  },
  "homepage": "https://github.com/MaxBocharnikov/excel-course#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.8.7",
    "eslint-plugin-jest": "^23.20.0",
    "normalize.css": "^8.0.1"
  }
